$build_arch_all = 1;
$apt_update = 1;
$apt_distupgrade = 1;
$run_lintian = 1;
$run_piuparts = 0;
$piuparts_require_success = 0;
$piuparts_opts = ['--schroot', '%r-%a-sbuild' ];
$piuparts_root_args = '';
$run_autopkgtest = 1;
$autopkgtest_require_success = 1;
$autopkgtest_root_args = '';
$autopkgtest_opts = [ '--no-auto-control', '--', 'schroot', '%r-%a-sbuild' ];
$lintian_opts = ['--suppress-tags', 'bad-distribution-in-changes-file,no-nmu-in-changelog,source-nmu-has-incorrect-version-number'];
$aspcud_criteria = '-removed,-changed,-new,-count(solution,APT-Release:=/o=Janitor/)';
$build_dep_resolver = 'aspcud';
$clean_source = 0;
$verbose = 1;
